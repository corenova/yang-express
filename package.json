{
  "name": "yang-express",
  "version": "0.6.0",
  "description": "YANG model-driven Express framework",
  "keywords": [
    "yang",
    "express",
    "model",
    "schema",
    "adaptive",
    "validate",
    "object",
    "middleware",
    "route",
    "router",
    "rfc6020"
  ],
  "author": "Peter Lee <peter@corenova.com>",
  "homepage": "https://github.com/corenova/yang-express",
  "license": "Apache-2.0",
  "repository": "corenova/yang-express",
  "main": "./yang-express.js",
  "bin": {
    "yang-express": "./bin/yang-express"
  },
  "preferGlobal": true,
  "yang": {
    "resolve": {
      "example-petstore": "./example/example-petstore.yang",
      "ietf-inet-types": "yang-js",
      "ietf-yang-library": "yang-js",
      "ietf-yang-types": "yang-js",
      "yang-meta-types": "yang-js",
      "yang-openapi": "yang-swagger",
      "yang-express": "./yang-express.js"
    }
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "debug": "^4.1.1",
    "express": "^4.14.0",
    "js-yaml": "^3.6.1",
    "yang-js": "^0.18.28",
    "yang-swagger": "^1.2.4"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "rimraf": "^2.5.2",
    "should": "~3.1.3"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean -s && mkdir -p dist",
    "build:api": "yang-swagger -f yaml -o swagger.yaml yang-express",
    "build": "npm run build:api",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha",
    "start": "bin/yang-express -r restjson yang-express",
    "example:petstore": "NODE_ENV=example NODE_APP_INSTANCE=petstore bin/yang-express --router restjson example-petstore"
  }
}
