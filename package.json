{
  "name": "yang-express",
  "version": "0.4.1",
  "description": "YANG model-driven Express framework",
  "keywords": [
    "yang",
    "express",
    "model",
    "schema",
    "adaptive",
    "validate",
    "object",
    "middleware",
    "route",
    "router",
    "rfc6020"
  ],
  "author": "Peter Lee <peter@corenova.com>",
  "homepage": "https://github.com/corenova/yang-express",
  "license": "Apache-2.0",
  "repository": "corenova/yang-express",
  "main": "lib/yang-express.js",
  "bin": {
    "yang-express": "./bin/yang-express"
  },
  "preferGlobal": true,
  "models": {
    "ietf-inet-types": "yang-js",
    "ietf-yang-library": "yang-js",
    "yang-meta-types": "yang-js",
    "yang-openapi": "yang-swagger",
    "yang-express": "./lib/yang-express.js"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "js-yaml": "^3.6.1",
    "socket.io": "^1.4.8",
    "yang-js": "^0.16.5",
    "yang-swagger": "^1.0.4"
  },
  "devDependencies": {
    "coffee-script": ">=1.7.0",
    "mocha": "~2.0.1",
    "rimraf": "^2.5.2",
    "should": "~3.1.3"
  },
  "engines": {
    "node": ">=0.10.28"
  },
  "scripts": {
    "clean": "rimraf dist/* lib/*",
    "prebuild": "npm run clean -s && mkdir -p dist",
    "build:src": "coffee -o lib -c src",
    "build:api": "yang-swagger -f yaml -o swagger.yaml yang-express",
    "build": "npm run build:src && npm run build:api",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha",
    "start": "bin/yang-express yang-express",
    "example:petstore": "DEBUG=yang:express NODE_ENV=example NODE_APP_INSTANCE=petstore bin/yang-express example/example-petstore.yang"
  }
}
