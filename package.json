{
  "name": "yang-express",
  "version": "0.4.0",
  "description": "YANG model-driven Express framework",
  "keywords": [
    "yang",
    "express",
    "model",
    "schema",
    "adaptive",
    "validate",
    "object",
    "middleware",
    "route",
    "router",
    "rfc6020"
  ],
  "author": "Peter Lee <peter@corenova.com>",
  "homepage": "https://github.com/corenova/yang-express",
  "license": "Apache-2.0",
  "repository": "corenova/yang-express",
  "main": "lib/yang-express.js",
  "models": {
    "ietf-inet-types": "yang-js",
    "ietf-yang-library": "yang-js",
    "yang-meta-types": "yang-js",
    "yang-express": "./lib/yang-express.js",
    "yang-openapi": "./schema/yang-openapi.yang"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "js-yaml": "^3.6.1",
    "socket.io": "^1.4.8",
    "yang-js": "^0.16.1"
  },
  "devDependencies": {
    "coffee-script": ">=1.7.0",
    "mocha": "~2.0.1",
    "rimraf": "^2.5.2",
    "should": "~3.1.3"
  },
  "engines": {
    "node": ">=0.10.28"
  },
  "scripts": {
    "clean": "rimraf dist/* lib/*",
    "prebuild": "npm run clean -s && mkdir -p dist",
    "build:src": "coffee -o lib -c src",
    "build": "npm run build:src",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha",
    "example:petstore": "npm run build && coffee ./example/petstore-express.coffee"
  }
}
